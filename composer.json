{
  "name": "drdplus/theurgist-calculator",
  "license": "MIT",
  "type": "project",
  "description": "Configurator of formulas for Drd+ theurgist",
  "autoload": {
    "psr-4": {
      "DrdPlus\\": "DrdPlus/"
    }
  },
  "require": {
    "php": ">=7.1",
    "drdplus/calculator-skeleton": "~1.0",
    "drdplus/theurgist-spells": "~1.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "~7.0",
    "mockery/mockery": "~1.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "pre-autoload-dump": [
      "rm -f DrdPlus/Tests/CalculatorSkeleton && ln -s ../../vendor/drdplus/calculator-skeleton/DrdPlus/Tests/CalculatorSkeleton DrdPlus/Tests/"
    ]
  }
}
